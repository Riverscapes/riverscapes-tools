  "version": "0.2.0",
  "inputs": [
    {
      "id": "environment",
      "type": "pickString",
      "description": "Select a RIVERSCAPES API environment",
      "options": [
        "staging",
        "production"
      ],
      "default": "production"
    },
    {
      "id": "cc_environment",
      "type": "pickString",
      "description": "Select a CYBERCASTOR API environment",
      "options": [
        "staging",
        "production"
      ],
      "default": "production"
    }
  ],
  "configurations": [
    {
      "name": "üì¶ Merge Projects Tool",
      "type": "debugpy",
      "request": "launch",
      "module": "riverscapes.merge-projects",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "{env:DATA_ROOT}/merge-projects",
      ]
    },
    {
      "name": "üß∞‚¨áÔ∏è DUMP Data Exchange to SQLite",
      "type": "debugpy",
      "request": "launch",
      "module": "riverscapes.lib.dump.dump_riverscapes",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "{env:DATA_ROOT}/warehouse_report/riverscapes_${input:environment}.gpkg",
        // "${input:cc_environment}",
        "production",
        "/Users/philipbailey/GISData/riverscapes/warehouse_report/watershed_boundary_template.gpkg",
      ]
    },
    {
      "name": "EXPERIMENTAL - Scrape Projects",
      "type": "debugpy",
      "request": "launch",
      "module": "riverscapes.downloadFiles",
