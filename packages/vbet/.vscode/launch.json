{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "inputs": [
    {
      "type": "promptString",
      "id": "HUC",
      "description": "What HUC?",
      // "default": "17070202" // North Fork John Day
      "default": "17060304", // Really small HUC
      // "default": "10190002" // VBET TAKING TOO LONG
      // "default": "17060103" // Asotin
    }
  ],
  "configurations": [
    {
      "name": "Run VBET",
      "type": "python",
      "request": "launch",
      "module": "vbet.vbet",
      "console": "integratedTerminal",
      "args": [
        "${input:HUC}",
        "STREAM_ORDER_TESTING",
        "FLOWLINES={env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/hydrology.gpkg/network,FLOW_AREA={env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/NHDArea.shp,SLOPE_RASTER={env:DATA_ROOT}/rs_context/${input:HUC}/topography/slope.tif,DEM={env:DATA_ROOT}/rs_context/${input:HUC}/topography/dem.tif,HILLSHADE={env:DATA_ROOT}/rs_context/${input:HUC}/topography/dem_hillshade.tif,CATCHMENTS={env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/NHDPlusCatchment.shp,WATERBODY={env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/NHDWaterbody.shp",
        "{env:DATA_ROOT}/vbet/${input:HUC}",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/nhd_data.sqlite/NHDPlusFlowlineVAA",
        //"--hand",
        //"{env:DATA_ROOT}/hand/${input:HUC}/outputs/hand.tif",
        "--reach_codes",
        "33400,46003,46006,46007,55800",
        //"--create_centerline",
        "--quick_mode",
        "--meta",
        "Runner=VSCode",
        "--verbose",
        //"--debug"
      ]
    },
    {
      "name": "Run Floodplain Connectivity (BETA)",
      "type": "python",
      "request": "launch",
      "module": "vbet.floodplain_connectivity",
      "console": "integratedTerminal",
      "args": [
        "{env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/hydrology.gpkg/network_intersected_300m",
        "{env:DATA_ROOT}/vbet/${input:HUC}/outputs/vbet.gpkg/vbet_50",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/transportation/roads.shp",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/transportation/railways.shp",
        "{env:DATA_ROOT}/vbet/${input:HUC}/connectivity",
        "--debug_gpkg",
        "{env:DATA_ROOT}/vbet/${input:HUC}/connectivity/debug.gpkg",
        "--verbose"
      ]
    },
    {
      "name": "Augment XML",
      "type": "python",
      "request": "launch",
      "module": "vbet.vbet_rs",
      "console": "integratedTerminal",
      "args": [
        "{env:DATA_ROOT}/vbet/${input:HUC}/project.rs.xml",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/project.rs.xml,{env:DATA_ROOT}/rs_context/${input:HUC}/project.rs.xml,{env:DATA_ROOT}/rs_context/${input:HUC}/project.rs.xml"
      ]
    },    
    {
      "name": "Write report",
      "type": "python",
      "request": "launch",
      "module": "vbet.vbet_report",
      "console": "integratedTerminal",
      "args": [
        "KW_TESTING",
        "{env:DATA_ROOT}/vbet/${input:HUC}/inputs/vbet_inputs.gpkg",
        "{env:DATA_ROOT}/vbet/${input:HUC}/outputs/vbet.gpkg",
        "{env:DATA_ROOT}/vbet/${input:HUC}/project.rs.xml",
        "{env:DATA_ROOT}/vbet/${input:HUC}/outputs/vbet_report.html"
      ]
    },    
    {
      "name": "Version Bump",
      "type": "python",
      "module": "rscommons.version",
      "args": [
        "${workspaceFolder}/vbet/__version__.py"
      ],
      "request": "launch",
      "console": "integratedTerminal"
    },
    {
      "name": "Floodplain Connectivity",
      "type": "python",
      "request": "launch",
      "module": "vbet.floodplain_connectivity",
      "console": "integratedTerminal",
    },
    {
      "name": "Transformation Curve",
      "type": "python",
      "request": "launch",
      "module": "vbet.scripts.transformation_curves",
      "console": "integratedTerminal",
    },
    {
      "name": "Sanitize Testing",
      "type": "python",
      "request": "launch",
      "module": "vbet.vbet_outputs",
      "console": "integratedTerminal"
    },
    { 
      "name": "HAND Testing",
      "type": "python",
      "request": "launch",
      "module": "scripts.hand",
      "console": "integratedTerminal"
    },
    { 
      "name": "Newtork",
      "type": "python",
      "request": "launch",
      "module": "vbet.vbet_network",
      "console": "integratedTerminal"
    },
    { 
      "name": "Vbet centerline",
      "type": "python",
      "request": "launch",
      "module": "vbet.vbet_centerline",
      "console": "integratedTerminal"
    },
    {
      "name": "VBET Observations",
      "type": "python",
      "request": "launch",
      "module": "scripts.vbet_evidence",
      "console": "integratedTerminal",
      "args": [
        "{env:GIS_PROJECTS_ROOT}/VBET_Evidence/postgis_export.gpkg/vbet_observations_huc",
        "{env:DATA_ROOT}/vbet",
        "{env:GIS_PROJECTS_ROOT}/VBET_Evidence/vbet_evidence.csv",
      ]
    }, 

  ]
}