{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "inputs": [
    {
      "type": "promptString",
      "id": "visitID",
      "description": "Which Visit ID?",
      "default": "213"
    }
  ],
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "CHaMP Metrics",
      "type": "debugpy",
      "module": "champ_metrics.run_champ_metrics",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "{env:DATA_ROOT}/champ/topo_projects/0213/TOPO_-_CHaMP-ASW00001-CC-F1_P2BR_with_aux/project.rs.xml",
        "{env:DATA_ROOT}/champ/metrics"
      ],
      "request": "launch",
      "console": "integratedTerminal"
    },
    {
      "name": "BATCH CHaMP Metrics",
      "type": "debugpy",
      "module": "champ_metrics.run_champ_metrics_batch",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "{env:CSV_FOLDER}",
        "{env:DATA_ROOT}/champ/topo_projects",
        "{env:DATA_ROOT}/champ/metrics"
      ],
      "request": "launch",
      "console": "integratedTerminal"
    },
    {
      "name": "Stream Temp",
      "type": "debugpy",
      "module": "champ_metrics.stream_temp",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "/Users/philipbailey/GISData/champ/stream_temp/whatever",
        "/Users/philipbailey/GISData/champ/stream_temp/champ_stream_temp.gpkg",
        "/Users/philipbailey/GISData/champ/stream_temp/champ_site_locations.json"
      ],
      "request": "launch",
      "console": "integratedTerminal"
    }
  ]
}