{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "inputs": [
    {
      "type": "promptString",
      "id": "HUC",
      "description": "What HUC?",
      // "default": "17070202" // North Fork John Day
      "default": "17060304" // Really small HUC
      // "default": "17060103" // Asotin
    }
  ],
  "configurations": [
    {
      "name": "Gnat",
      "type": "python",
      "request": "launch",
      "module": "gnat.gnat_rs",
      "console": "integratedTerminal",
      // Add NATIONAL_PROJECT, INPUTS_FOLDER, DOWNLOAD_FOLDER to your .env file
      "args": [
        "${input:HUC}",
        "{env:DATA_ROOT}/vbet/${input:HUC}/outputs/vbet.gpkg/vbet_50_sections",
        "{env:DATA_ROOT}/gnat/${input:HUC}",
        "flowlines={env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/hydrology.gpkg/network,segmented_flowlines={env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/hydrology.gpkg/network_intersected_300m",
        "--epsg", "5070",
        //"--meta",
        "--verbose"
      ]
    },
    {
      "name": "Write Confinement report",
      "type": "python",
      "request": "launch",
      "module": "sqlbrat.brat_report",
      "console": "integratedTerminal",
      "args": [
        "{env:DATA_ROOT}/confinement/${input:HUC}/outputs/brat.sqlite",
        "{env:DATA_ROOT}/confinement/${input:HUC}/project.rs.xml",
        "{env:DATA_ROOT}/confinement/${input:HUC}/outputs/brat_report.html"
      ]
    },
    {
      "name": "Version Bump",
      "type": "python",
      "module": "rscommons.version",
      "args": [
        "${workspaceFolder}/gnat/__version__.py"
      ],
      "request": "launch",
      "console": "integratedTerminal"
    },
    {
      "name": "Confinement",
      "type": "python",
      "request": "launch",
      "module": "gnat.confinement",
      "console": "integratedTerminal",
      // Add NATIONAL_PROJECT, INPUTS_FOLDER, DOWNLOAD_FOLDER to your .env file
      "args": [
        "${input:HUC}",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/hydrology.gpkg/network_intersected_300m",
        "{env:DATA_ROOT}/channel_area/${input:HUC}/outputs/channel_area.gpkg/channel_area",
        "{env:DATA_ROOT}/vbet/${input:HUC}/outputs/vbet.gpkg/vbet_full",
        "{env:DATA_ROOT}/confinement/${input:HUC}", // output folder
        "BFwidth",
        "ValleyBottom",
        "--meta",
        "Runner=VSCode",
        "--reach_codes",
        "33400,33600,33601,33603,46000,46003,46006,46007,55800",
        "--verbose",
        //"--debug"
      ]
    },
    {
      "name": "Augment Confinement XML",
      "type": "python",
      "request": "launch",
      "module": "gnat.confinement_rs",
      "console": "integratedTerminal",
      "args": [
        "{env:DATA_ROOT}/confinement/${input:HUC}/project.rs.xml",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/project.rs.xml,{env:DATA_ROOT}/vbet/${input:HUC}/project.rs.xml"
      ]
    },
    {
      "name": "Sinuosity",
      "type": "python",
      "request": "launch",
      "module": "gnat.sinuosity",
      "console": "integratedTerminal",
      "args": [
        "${input:HUC}",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/hydrology.gpkg/network_intersected_300m",
        "{env:DATA_ROOT}/vbet/${input:HUC}/outputs/vbet.gpkg/vbet_50",
        "{env:DATA_ROOT}/gnat/${input:HUC}", // output folder
        "--meta",
        "Runner=VSCode",
        "--reach_codes",
        "33400,33600,33601,33603,46000,46003,46006,46007,55800",
        "--verbose"
      ]
    },
    {
      "name": "Gradient",
      "type": "python",
      "request": "launch",
      "module": "gnat.gradient",
      "console": "integratedTerminal",
      "args": [
        "${input:HUC}",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/hydrology.gpkg/network_intersected_300m",
        "{env:DATA_ROOT}/vbet/${input:HUC}/outputs/vbet.gpkg/vbet_50",
        "{env:DATA_ROOT}/gnat/${input:HUC}", // output folder
        "--meta",
        "Runner=VSCode",
        "--reach_codes",
        "33400,33600,33601,33603,46000,46003,46006,46007,55800",
        "--verbose"
      ]
    },
    {
      "name": "Network Structure",
      "type": "python",
      "request": "launch",
      "module": "gnat.network_structure",
      "console": "integratedTerminal",
      "args": [
        "${input:HUC}",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/hydrology.gpkg/network_intersected_300m",
        "{env:DATA_ROOT}/vbet/${input:HUC}/outputs/vbet.gpkg/vbet_50",
        "{env:DATA_ROOT}/gnat/${input:HUC}", // output folder
        "--meta",
        "Runner=VSCode",
        "--reach_codes",
        "33400,33600,33601,33603,46000,46003,46006,46007,55800",
        "--verbose"
      ]
    },
    {
      "name": "Zonal Attributes - Polygons",
      "type": "python",
      "request": "launch",
      "module": "gnat.zonal_attributes",
      "console": "integratedTerminal",
      "args": [
        "${input:HUC}",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/hydrology.gpkg/NHDPlusCatchments",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/ownership/ownership.gpkg/ownership",
        "ADMIN_AGEN",
        "{env:DATA_ROOT}/gnat/${input:HUC}/gnat_output.gpkg",
        "--epsg",
        "5070"
      ]
    },
    {
      "name": "Zonal Attributes - Lines",
      "type": "python",
      "request": "launch",
      "module": "gnat.zonal_attributes",
      "console": "integratedTerminal",
      "args": [
        "${input:HUC}",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/hydrology.gpkg/NHDPlusCatchments",
        "{env:DATA_ROOT}/gnat/hydrology.gpkg/network",
        "FCode",
        "{env:DATA_ROOT}/gnat/${input:HUC}/gnat_output.gpkg",
        "--epsg",
        "5070"
      ]
    },
    {
      "name": "Zonal Attributes - Network Nodes",
      "type": "python",
      "request": "launch",
      "module": "gnat.zonal_attributes",
      "console": "integratedTerminal",
      "args": [
        "${input:HUC}",
        "{env:DATA_ROOT}/rs_context/${input:HUC}/hydrology/hydrology.gpkg/NHDPlusCatchments",
        "{env:DATA_ROOT}/gnat/hydrology.gpkg/nodes",
        "none",
        "{env:DATA_ROOT}/gnat/${input:HUC}/gnat_output.gpkg",
        "--epsg",
        "5070"
      ]
    },
  ]
}